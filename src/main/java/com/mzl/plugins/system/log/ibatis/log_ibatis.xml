<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="log">

  <parameterMap id="log" type="com.mzl.plugins.log.entity.Log" />
  
  <resultMap id="logResult" type="com.mzl.plugins.log.entity.Log">
    <result property="id" column="ID" javaType="java.lang.Long" />
   
  </resultMap>
	
  <select id="getRecordCount" parameterMap="log" resultType="Long">
  	 SELECT count(1) FROM SM_Log
  </select>

</mapper>